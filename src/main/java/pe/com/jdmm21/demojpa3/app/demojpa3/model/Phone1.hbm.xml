<hibernate-mapping>
    <class name="pe.com.jdmm21.demojpa3.app.demojpa3.model.Phone1" table="phone">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="phoneNumber" column="phonenumber"/>
        <one-to-one name="person1"
            class="pe.com.jdmm21.demojpa3.app.demojpa3.model.Person1"
            cascade="all"/>
    </class>
</hibernate-mapping>